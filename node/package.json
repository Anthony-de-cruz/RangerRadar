{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start.dev": "nodemon -L",
    "test": "jest --verbose",
    "e2e": "cypress open"
  },
  "dependencies": {
    "@vonage/server-sdk": "^3.14.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^7.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.11.5",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "cypress": "^13.11.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.3"
  },
  "nodemonConfig": {
    "ext": "js,json,ts,html,pug,css,",
    "watch": [
      "./*"
    ],
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "_exec": "ts-node ./src/index.ts",
    "verbose": true,
    "legacyWatch": true,
    "env": {
      "NODE_ENV": "development"
    }
  }
}
